         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Heated Slabs Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">All heated slabs</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">All heated slabs shall be insulated as required by Section 110.8(g).  Footings must meet required insulation levels.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation installed from the top of the slab</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation shall be installed from the top of the slab, down 16 inches or to the frost line, whichever is greater.  Climate zones 1-15 require R-5, and climate zone 16 requires R-10.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Alternative vertical insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Alternatively, vertical insulation from top of slab at inside edge of outside wall down to the top of the horizontal insulation. Horizontal insulation from the outside edge of the vertical insulation extending 4 feet toward the center of the slab in a direction normal to the outside of the building in plain view. Climate zones 1-5 require R-5, and climate zone 16 requires R-10 vertical and R-7 horizontal.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Minimum Mandatory Measures</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation - 110.8(a)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Insulation[/d:b] - 110.8(a): All installed Insulation is certified and listed with Department of Consumer Affairs, Bureau of Household Goods and Services.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation - 110.8(b)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Insulation[/d:b] - 110.8(b): Urea formaldehyde foam insulation is protected by 4 mil polyethylene vapor retarder.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation - 110.8(c)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Insulation[/d:b] - 110.8(c): Flame spread and smoke density requirements of the California Building Code (CBC) are met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Raised Floor - 150.0(d)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Raised Floor[/d:b] - 150.0(d): All raised wood-frame floor have a minimum R-19 insulation or equivalent U-factor</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Slab Floor/Perimeter - 150.0(l)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Slab Floor/Perimeter[/d:b] - 150.0(l): Water absorption rate for the insulation material alone without facings is no greater than 0.3%; water vapor permeance rate is no greater than 2.0 perm/inch and is protected from physical damage and UV light deterioration.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Above Grade Exterior Wall - 150.0(c)1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Above Grade Exterior Wall[/d:b] - 150.0(c)1 and 150.1(c)6: All 2x4 wood-frame walls have a minimum R-13 insulation or equivalent U-factor not exceeding U-0.102.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Above Grade Exterior Wall - 150.0(c)2</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Above Grade Exterior Wall[/d:b] - 150.0(c)2 and 150.1(c)6: All 2x6 wood-frame walls have a minimum R-20 insulation or equivalent U-factor not exceeding U-0.071.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Roof Deck</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b] Roof Deck[/d:b] - 150.0(a)1: All newly constructed attic systems have a maximum area-weighted average U-factor not exceeding U-0.184.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ceiling/Rafter Roof - 150.0(a)2</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Ceiling/Rafter Roof[/d:b] - 150.0(a)2: All wood-frame ceiling have a minimum R-22 insulation or equivalent U-factor.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Vapor Retarder - 150(g)1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Vapor Retarder[/d:b] - 150(g)1:  In Climate Zones 1 through 16, the earth floor of unvented crawl space shall be covered with a Class I or Class II vapor retarder. This requirement shall also apply to controlled ventilation crawl space for buildings complying with the Exception to Section 150.0(d).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G11" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Vapor Retarder - 150(g)2</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Vapor Retarder[/d:b] - 150(g)2:  In Climate Zones 14 and 16, a Class I or Class II vapor retarder shall be installed on the conditioned space side of all insulation in all exterior walls, vented attics and unvented attics with air-permeable insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Heated Slabs - 110.8(g)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Heated Slabs[/d:b] - 110.8(g): All heated slabs shall be insulated as required. [d:l t="*"][d:list] Insulation shall be installed from the top of the slab, down 16 inches or to the frost line, whichever is greater.  Climate zones 1-15 require R-5, and climate zone 16 requires R-10. [/d:list][d:list] Alternatively, vertical insulation from top of slab at inside edge of outside wall down to the top of the horizontal insulation. Horizontal insulation from the outside edge of the vertical insulation extending 4 feet toward the center of the slab in a direction normal to the outside of the building in plain view. Climate zones 1-15 require R-5, and  climate zone 16 requires R-10 vertical and R-7 horizontal. [/d:list][/d:l]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="GEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Installed insulation R-values</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Installed insulation R-values are the same or greater than listed on the CF1R.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">No gaps</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">No gaps or voids between the insulation and framing.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">No gaps</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">No gaps between the sides or ends of batt insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Loose-fill insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Loose-fill insulation must be installed to the minimum installed weight per square foot (density) of the manufacturer's cut sheet for the proposed R-value.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Batt insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Batt insulation is not compressed (no stuffing of the insulation into the cavity) and is installed to its full thickness.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation is cut around obstructions</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation is cut around obstructions such as electrical boxes.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Batt insulation delaminated</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Batt insulation is delaminated around all plumbing and electrical lines in ceilings, walls and floors.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Band joists</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Band joists are insulated to the same R-value as the wall.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Narrow cavities</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">In all narrow cavities the insulation shall be cut to fit or filled with expanding foam.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Manufacturer instructions</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation was installed per manufacturer instructions.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Wall Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="I01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Low expanding foam</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">When allowed by manufacturer, low expanding foam shall be used to fill gaps and voids around windows and doors. If not, the cavity must be air tight and filled completely with insulation.  Batt insulation must be cut to width.  No stuffing allowed.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation before Tub, Shower, Fireplace</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Install wall insulation before installing tubs, showers and fireplaces.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electric Panel sealed and insulated</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Electric panels on walls separating conditioned and unconditioned space are sealed and insulated behind the panel with rigid insulation or expanding foam.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Walls of interior closets vented to the outside</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">All walls of interior closets vented to the outside for HVAC or water heating equipment have the same R-value and air barrier as the exterior walls and ceiling.  Doors are insulated and weather stripped.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ducting R-value in exterior walls</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Ducting not allowed in exterior walls unless insulated to R6 or greater and the insulation and ducting are not crushed.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Corner channels, wall intersections, and double sided shear walls</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Corner channels, wall intersections, and double sided shear walls insulated to the required R-value before enclosing the wall.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exterior air barrier</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation that does not fill the cavity placed against exterior air barrier.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Band joists</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Band joists are insulated to the same R-value as the walls.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="IEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Ceiling/ Roof Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exterior top plates</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation extends to the outside edge of the exterior top plates and is flush against any ventilation dams/baffles.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">No gaps between the ceiling and the insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation is in direct contact with ceiling, so there are no gaps between the ceiling and the insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Chimneys and flues</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">For chimneys and flues, the insulation is in contact with the sheet metal collar.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Can lights</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Can lights are covered with insulation to the same depth as required by the CF1R for ceiling insulation.  If not, an area weighted calculation is required to be turned in with this compliance document (CF1R-ENV-02-E).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Walkways and mechanical platforms insulated</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Walkways and mechanical platforms insulated to the same R-value as required for the ceiling. If not, an area weighted calculation is required to be turned in with this compliance document (CF1R-ENV-02-E).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulate soffits</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulate soffits by adding an air barrier and cover with insulation, or insulate the entire soffit including floor and walls.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Knee walls and skylight shafts</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Knee walls and skylight shafts are insulated to the wall R-value and in full contact with the interior air barrier.  If framing on these surfaces is laid flat batt insulation is cut to fit around the framing.  Batt insulation is not allowed to be draped over the framing.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Attic access doors</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Attic access doors insulated to the same R-value as ceiling. The insulation is permanently attached using adhesive or mechanical fasteners.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Attic access surrounded with a dam</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Attic access must be surrounded with a dam at least the same depth as the insulation to prevent loss of ceiling insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Batt insulation cut to fit</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Batt insulation cut to fit around cross bracings and truss webs in attic.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Raised Floor Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Subfloor insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation in full contact with subfloor.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation hangers</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation hangers spaced at 18 inches or less, insulation hangers must not compress insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Netting or mesh</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If netting or mesh is used, the cavity under the floor is filled and in contact with the subfloor.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Basement</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If the basement is conditioned the walls adjacent to the crawlspace must meet minimum wall R-value requirements.  This includes framed stem walls, and vertical concrete retaining walls.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Access to the crawl space</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If access to the crawl space is from the conditioned area, the raised floor must have an airtight insulated access hatch.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_L" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Floor Above Garage Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="L01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Subfloor insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation must be in full contact with subfloor if the air barrier is at the band joist at the garage/house wall.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation hangers</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation hangers spaced at 18 inches or less, insulation hangers must not compress insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Netting or mesh</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If netting or mesh is used, the cavity under the floor is filled and in contact with the subfloor.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air barrier at the perimeter</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If air barrier is at the perimeter of the garage, below conditioned subfloor, the insulation is placed on the garage ceiling.  The perimeter of subfloor is also insulated.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_M" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Cantilevered Floor Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="M01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cantilevered subfloor insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation in full contact with cantilevered subfloor.  Insulation hangers spaced at 18 inches or less, insulation hangers do not compress insulation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Netting or mesh</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If netting or mesh is used, the cavity under the floor is filled and in contact with the subfloor.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Sealed Blocking</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Sealed blocking is installed between joists where wall rim joist would be located in the absence of a cantilever.  Insulation is placed on both sides of this block.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="MEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_N" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Attached Porch Roof Insulation</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="N01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exterior insulated wall</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The exterior insulated wall at the intersection with the porch roof is fully insulated above, below and behind the roof line.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Truss framing</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Where truss framing is used, airtight blocking is at the top and bottom of each wall/roof section and insulated.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="NEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="cF2RENV03E" type="CF2RENV03E">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the CF2RENV03E compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
