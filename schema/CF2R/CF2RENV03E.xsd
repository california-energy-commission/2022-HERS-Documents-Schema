<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/CF2RENV03E"
   xmlns="http://www.lmonte.com/besm/CF2RENV03E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="CF2RENV03E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF INSTALLATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Insulation Installation"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderCF2R_3R" />
                     <xsd:element ref="cF2RENV03E" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF2R_E" />
                     <xsd:element name="Footer" type="comp:Footer" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="CF2RENV03E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="CF2RENV03E">
      <xsd:annotation>
         <xsd:documentation>Insulation Installation</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Roof/Ceiling Insulation</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if the CF1R contains a roof/ceiling entry.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableRoof" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="A01_PartitionName" type="env:PartitionName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">I.D.</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data from CF1R</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A02_ManufacturerBrand" type="env:ManufacturerBrand" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Manufacturer and Brand</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">user input text; maximum 28 characters</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A03_PartitionFramingMaterial" type="env:PartitionFramingMaterial" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly/Framing Material</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If data is from prescriptive CF1R , use PartitionFramingMaterial to store values of Metal or WoodFraming, Else if data is from performance CF1R use RoofFramingMaterialResCBECC to store values of Built-up Roof, SIPS Ceiling, Wood Framed Ceiling; Else if not applicable use the  NotApplicableMessage to store N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A03_RoofFramingMaterialCBECC_Res" type="env:RoofFramingMaterialCBECC_Res" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly/Framing Material</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If data is from prescriptive CF1R , use PartitionFramingMaterial to store values of Metal or WoodFraming, Else if data is from performance CF1R use RoofFramingMaterialResCBECC to store values of Built-up Roof, SIPS Ceiling, Wood Framed Ceiling; Else if not applicable use the  NotApplicableMessage to store N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly/ Framing Material</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If data is from prescriptive CF1R , use PartitionFramingMaterial to store values of Metal or WoodFraming, Else if data is from performance CF1R use RoofFramingMaterialResCBECC to store values of Built-up Roof, SIPS Ceiling, Wood Framed Ceiling; Else if not applicable use the  NotApplicableMessage to store N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A04_Thickness" type="com:Thickness" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly Thickness (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data from CF1R, Else result is N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly Thickness (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data from CF1R, Else result is N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A05_FramingDescriptionCBECC_Res" type="env:FramingDescriptionCBECC_Res" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Framing Size and Spacing</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data from CF1R, Else result is N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Framing Size and Spacing</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data from CF1R, Else result is N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="A06_InsulationType" type="env:InsulationType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Insulation Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list of insulation types: Cellulose, Cement EPS compound, Expanded polystyrene, Extruded polystyrene, Fiberglass, Insulation other, Mineral wool, Plastic polyisocyanurate, Plastic polystyrene, Polyurethane, Spray Foam Insulation (SPF), Urea formaldehyde;   If the insulation they used is not listed they should select Insulation Other.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A07_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Ceiling Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A08_InsulationRValue</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Ceiling Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A08_NotApplicableMessage</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A08_InsulationThickness" type="env:InsulationThickness" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Depth (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A08_NotApplicableMessage == N/A then result is N/A stored in the NotApplicableMessage, Else User input number</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A09_InsulationThickness</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Depth (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A08_NotApplicableMessage == N/A then result is N/A stored in the NotApplicableMessage, Else User input number</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A09_NotApplicableMessage</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A09_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Insulation Above the Roof Deck R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if numeric value ), but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A10_InsulationRValue</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A09_ContinuousInsulationString" type="env:ContinuousInsulationString" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Insulation Above the Roof Deck R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R PRF (use this choice if string) but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A10_ContinuousInsulationString</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Insulation Above the Roof Deck R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (string or DecimalNonnegative) but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">A10_NotApplicableMessage</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A10_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Below the Roof Deck R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if DecimalNonnegative) but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A10_ContinuousInsulationString" type="env:ContinuousInsulationString" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Below the Roof Deck R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if string) but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Below the Roof Deck R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (string or DecimalNonnegative) but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Wall Insulation</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If the CF1R contains a wall entry,</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableWall" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_PartitionName" type="env:PartitionName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">I.D.</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data from CF1R</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B02_ManufacturerBrand" type="env:ManufacturerBrand" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Manufacturer and Brand</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input, text; maximum 28 characters</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B03_PartitionFramingMaterial" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly/Framing Material</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If data is from prescriptive CF1R , use PartitionFramingMaterial to store values for Metal or WoodFraming, Else if data is from performance CF1R use WallFramingMaterialResCBECC to store values for SIPS  Walls, Steel Framed Walls, Wood Framed Walls;</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="env:PartitionFramingMaterial">
                                                <xsd:enumeration value="Metal"/>
                                                <xsd:enumeration value="WoodFraming"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="B03_WallFramingMaterialCBECC_Res" type="env:WallFramingMaterialCBECC_Res" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly/Framing Material</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If data is from prescriptive CF1R , use PartitionFramingMaterial to store values for Metal or WoodFraming, Else if data is from performance CF1R use WallFramingMaterialResCBECC to store values for SIPS  Walls, Steel Framed Walls, Wood Framed Walls;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B04_Thickness" type="com:Thickness" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly Thickness (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data from CF1R, Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Assembly Thickness (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data from CF1R, Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B05_FramingDescriptionCBECC_Res" type="env:FramingDescriptionCBECC_Res" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Framing Size and Spacing</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data from CF1R</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B06_InsulationType" type="env:InsulationType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Insulation Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list of insulation types: Cellulose, Cement EPS compound, Expanded polystyrene, Extruded polystyrene, Fiberglass, Insulation other, Mineral wool, Plastic polyisocyanurate, Plastic polystyrene, Polyurethane, Spray Foam Insulation (SPF), Urea formaldehyde;   If the insulation they used is not listed they should select Insulation Other.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Core/Cavity Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B08_InsulationRValue</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Core/Cavity Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B08_NotApplicableMessage</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B08_InsulationThickness" type="env:InsulationThickness" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Depth (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input number or result = N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B09_InsulationThickness</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Insulation Depth (inches)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input number or result = N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B09_NotApplicableMessage</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B09_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Exterior Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if numeric value ), but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B10_InsulationRValue</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B09_ContinuousInsulationString" type="env:ContinuousInsulationString" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">ContinuousExterior Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if string value ), but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B10_ContinuousInsulationString</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Exterior Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (string or DecimalNonmegative) but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                             <xsd:appinfo source="PrevName">
                                                <dtyp:prevSchema version="2019.1.006" changed="cell number">B10_NotApplicableMessage</dtyp:prevSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B10_InsulationRValue" type="env:InsulationRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Interior Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if numeric value ), but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_ContinuousInsulationString" type="env:ContinuousInsulationString" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Continuous Interior Insulation R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled from CF1R (use this choice if string value ), but user can override with value GTE the CF1R value; Else result is N/A stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
